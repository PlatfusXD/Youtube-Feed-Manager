{
  "name": "YouToube Feed Manager",
    "version": "1.0.3",
    "manifest_version": 2,
    "description": "A chromium Extension to manage youtube feed",
    "icons": {
        "512": "img/logo-sm.png"
    },
	"options_page": "index.html",
    "browser_action": {
        "default_popup": "popup/window.html"
    },
	"background": {
    "scripts": ["menu.js"],
	"run_at": "document_start",
	"persistent": true
	},
	"permissions": ["storage",
	"contextMenus", 
      "tabs",
	  "webNavigation"],
	"content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["background.js"],
	"run_at": "document_end"
  }
  
]
}